<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

 <plugin>

  <!-- EEF extensions -->
   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <factory
            uri="http://org/msee/slmtoolbox/modeling/model/ssm/tim.ecore/HumanRelatedPart"
            class="org.msee.slmtoolbox.modeling.model.ssm.tim.HumanRelatedPart.providers.HumanRelatedPartEEFAdapterFactory"
            supportedTypes="org.eclipse.emf.eef.runtime.providers.PropertiesEditingProvider"/>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.msee.slmtoolbox.modeling.model.ssm.tim.HumanRelatedPart.providers.HumanRelatedPartPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.msee.slmtoolbox.modeling.model.ssm.tim.ITRelatedPart.providers.ITRelatedPartPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.msee.slmtoolbox.modeling.model.ssm.tim.providers.TimPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.msee.slmtoolbox.modeling.model.ssm.tim.CommonConcepts.providers.CommonConceptsPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.msee.slmtoolbox.modeling.model.ssm.tim.PhysicalMeanRelatedPart.providers.PhysicalMeanRelatedPartPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>

  <!-- Tabbed properties views extension -->
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
      <propertyContributor
            contributorId="org.msee.slmtoolbox.modeling.model.ssm.tim.HumanRelatedPart.properties"
            labelProvider="org.eclipse.emf.eef.runtime.ui.utils.EEFLabelProvider">
         <propertyCategory
               category="default">
         </propertyCategory>
         <propertyCategory
               category="extended">
         </propertyCategory>
         <propertyCategory
               category="advanced">
         </propertyCategory>
      </propertyContributor>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs
            contributorId="org.msee.slmtoolbox.modeling.model.ssm.tim.HumanRelatedPart.properties">
         <propertyTab
               id="Base"
               label="Base"
               category="default">
         </propertyTab>

      </propertyTabs>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="org.msee.slmtoolbox.modeling.model.ssm.tim.HumanRelatedPart.properties">
         <propertySection
               class="org.msee.slmtoolbox.modeling.model.ssm.tim.HumanRelatedPart.parts.forms.TimHumanPropertiesEditionPartForm"
               filter="org.msee.slmtoolbox.modeling.model.ssm.tim.HumanRelatedPart.providers.TimHumanPropertiesEditionProvider$EditionFilter"
               id="org.msee.slmtoolbox.modeling.model.ssm.tim.HumanRelatedPart.section.TimHuman"
               tab="Base">
         </propertySection>
         <propertySection
               class="org.msee.slmtoolbox.modeling.model.ssm.tim.HumanRelatedPart.parts.forms.TimOrganizationPropertiesEditionPartForm"
               filter="org.msee.slmtoolbox.modeling.model.ssm.tim.HumanRelatedPart.providers.TimOrganizationPropertiesEditionProvider$EditionFilter"
               id="org.msee.slmtoolbox.modeling.model.ssm.tim.HumanRelatedPart.section.TimOrganization"
               tab="Base">
         </propertySection>
         <propertySection
               class="org.msee.slmtoolbox.modeling.model.ssm.tim.HumanRelatedPart.parts.forms.TimOrganizationUnitPropertiesEditionPartForm"
               filter="org.msee.slmtoolbox.modeling.model.ssm.tim.HumanRelatedPart.providers.TimOrganizationUnitPropertiesEditionProvider$EditionFilter"
               id="org.msee.slmtoolbox.modeling.model.ssm.tim.HumanRelatedPart.section.TimOrganizationUnit"
               tab="Base">
         </propertySection>
      </propertySections>
   </extension>
